<?xml version="1.0"?>
<launch>
	<include file="$(find asv_gazebo)/launch/gazebo.launch" />

	<node name="slam" pkg="gmapping" type="slam_gmapping">

	</node>

	<node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
		<rosparam file="$(find asv_navigation)/config/global_map_config.yaml" command="load" ns="global_costmap" />
		<rosparam file="$(find asv_navigation)/config/local_map_config.yaml" command="load" ns="local_costmap" />
		<rosparam file="$(find asv_navigation)/config/local_planner.yaml" command="load" />
	</node>

</launch>